<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>selector</managed-bean-name>
        <managed-bean-class>Selector</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
     <managed-bean>
        <managed-bean-name>util</managed-bean-name>
        <managed-bean-class>Util</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>customer</managed-bean-name>
        <managed-bean-class>Customer</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>    
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>Login</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>  
    <navigation-rule>
        <from-view-id>
            /index.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>start.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>
            /start.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>newDoctor</from-outcome>
            <to-view-id>newDoctor.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>listDoctors</from-outcome>
            <to-view-id>allDoctors.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>findDoctor</from-outcome>
            <to-view-id>findDoctor.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteDoctor</from-outcome>
            <to-view-id>deleteDoctor.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>newTechnician</from-outcome>
            <to-view-id>newTechnician.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>listTechnicians</from-outcome>
            <to-view-id>allTechnicians.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>findTechnician</from-outcome>
            <to-view-id>findTechnician.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>deleteTechnician</from-outcome>
            <to-view-id>deleteTechnician.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /newDoctor.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>start.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /newTechnician.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>start.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /findDoctor.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>showCustomer</from-outcome>
            <to-view-id>displayCustomer.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /findTechnician.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>showCustomer</from-outcome>
            <to-view-id>displayCustomer.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /deleteDoctor.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>start.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>
            /deleteTechnician.xhtml
        </from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>start.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    
    
</faces-config>
